<%@page import="service.NewPassword"%>
<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%@ include file="header.jsp" %>

	<jsp:useBean id="input" class="beans.AccountVO"/>
	<jsp:setProperty property="*" name="input"/>

	<%
		AccountVO find = dao.findPw(input);
		"String "
		
		String pw = NewPassword.getPassword();
		find.setUserpw(pw);
		
		int row = dao.update(find);		
	%>
	
	<ul>
		<li><%=find.getIdx() %></li>
		<li><%=find.getUserid()%></li>
		<li><%=find.getUserpw() %></li>
		<li><%=find.getNick() %></li>
		<li><%=find.getEmail() %></li>
		<li><%=find.getJoin_date() %></li>
	</ul>

